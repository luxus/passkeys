{
  "name": "@passlock/client",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "author": {
    "name": "Toby Hobson",
    "email": "toby@passlock.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/passlock-dev/passkeys",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./src/": "./src/"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite --host --open",
    "test": "vitest",
    "test:ui": "vitest --coverage.enabled=true --ui",
    "coverage": "vitest run --coverage",
    "build": "pnpm run clean && tsc",
    "watch": "tsc && vite build --watch",
    "typecheck": "tsc -noEmit",
    "lint": "eslint --ext .ts src",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write \"src/**/*.+(js|ts|json)\""
  },
  "dependencies": {
    "@effect/schema": "^0.60.4",
    "@github/webauthn-json": "^2.1.1",
    "@passlock/shared": "file:../shared",
    "effect": "2.1.2",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "dts-bundle-generator": "^9.2.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jsdom": "^23.2.0",
    "node20": "link:@tsconfig/node20",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.1"
  }
}
