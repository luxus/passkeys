{
  "name": "@passlock/passkeys",
  "version": "0.8.0",
  "description": "",
  "keywords": [],
  "author": {
    "name": "Toby Hobson",
    "email": "toby@passlock.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/passlock-dev/passkeys",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite dev",
    "test": "vitest run",
    "test:watch": "vitest dev",
    "test:ui": "vitest --coverage.enabled=true --ui",
    "test:coverage": "vitest run --coverage",
    "build": "pnpm run clean && tsc --build",
    "watch": "tsc --build --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .ts src",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write \"src/**/*.+(js|ts|json)\"",
    "publish": "pnpm publish --no-git-checks --access public",
    "ncu": "ncu -x @passlock/shared",
    "ncu:save": "ncu -u -x @passlock/shared"
  },
  "dependencies": {
    "@effect/schema": "^0.62.5",
    "@github/webauthn-json": "^2.1.1",
    "@passlock/shared": "workspace:*",
    "effect": "2.3.4",
    "lodash-es": "^4.17.21",
    "valibot": "^0.28.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "dts-bundle-generator": "^9.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vitest": "^1.2.2",
    "vitest-mock-extended": "^1.3.1"
  }
}
